{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Python Flask avec Redis",
        "type": "python",
        "request": "launch",
        "module": "flask",
        "env": {
          "FLASK_APP": "app.py",
          "FLASK_DEBUG": "1"
        },
        "args": [
          "run",
          "--no-debugger",
          "--no-reload",
          "--port",
          "5522"  // Specify the port here
        ],
        "jinja": true,
        "justMyCode": true
      }
    ]
  }
  